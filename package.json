    "@trivago/prettier-plugin-sort-imports": "5.2.2"
  },
  "devDependencies": {
    "chokidar-cli": "3.0.0",
    "np": "10.2.0",
    "prettier": "3.6.2",
    "yalc": "1.0.0-pre.53"
